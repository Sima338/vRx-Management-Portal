    "dashboard": {
      "name": "dashboard",
      "type": "app",
      "data": {
        "root": "vrx-mf/apps/dashboard",
        "name": "dashboard",
        "$schema": "../../node_modules/nx/schemas/project-schema.json",
        "projectType": "application",
        "prefix": "vrx",
        "sourceRoot": "apps/dashboard/src",
        "tags": [],
        "targets": {
          "build": {
            "executor": "@nx/angular:webpack-browser",
            "outputs": [
              "{options.outputPath}"
            ],
            "options": {
              "outputPath": "dist/apps/dashboard",
              "index": "apps/dashboard/src/index.html",
              "main": "apps/dashboard/src/main.ts",
              "polyfills": [
                "zone.js"
              ],
              "tsConfig": "apps/dashboard/tsconfig.app.json",
              "inlineStyleLanguage": "scss",
              "assets": [
                {
                  "glob": "**/*",
                  "input": "apps/dashboard/public"
                }
              ],
              "styles": [
                "apps/dashboard/src/styles.scss"
              ],
              "customWebpackConfig": {
                "path": "apps/dashboard/webpack.config.ts"
              }
            },
            "configurations": {
              "production": {
                "budgets": [
                  {
                    "type": "initial",
                    "maximumWarning": "500kb",
                    "maximumError": "1mb"
                  },
                  {
                    "type": "anyComponentStyle",
                    "maximumWarning": "4kb",
                    "maximumError": "8kb"
                  }
                ],
                "outputHashing": "all",
                "customWebpackConfig": {
                  "path": "apps/dashboard/webpack.prod.config.ts"
                }
              },
              "development": {
                "buildOptimizer": false,
                "optimization": false,
                "vendorChunk": true,
                "extractLicenses": false,
                "sourceMap": true,
                "namedChunks": true
              }
            },
            "defaultConfiguration": "production",
            "parallelism": true
          },
          "serve": {
            "continuous": true,
            "executor": "@nx/angular:dev-server",
            "options": {
              "port": 4201,
              "publicHost": "http://localhost:4201"
            },
            "configurations": {
              "production": {
                "buildTarget": "dashboard:build:production"
              },
              "development": {
                "buildTarget": "dashboard:build:development"
              }
            },
            "defaultConfiguration": "development",
            "dependsOn": [
              "shell:serve"
            ],
            "parallelism": true
          },
          "extract-i18n": {
            "executor": "@angular-devkit/build-angular:extract-i18n",
            "options": {
              "buildTarget": "dashboard:build"
            },
            "configurations": {},
            "parallelism": true
          },
          "lint": {
            "executor": "@nx/eslint:lint",
            "configurations": {},
            "options": {},
            "parallelism": true
          },
          "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
              "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
              "jestConfig": "apps/dashboard/jest.config.ts",
              "tsConfig": "apps/dashboard/tsconfig.spec.json"
            },
            "configurations": {},
            "parallelism": true
          },
          "serve-static": {
            "continuous": true,
            "executor": "@nx/web:file-server",
            "defaultConfiguration": "production",
            "options": {
              "buildTarget": "dashboard:build",
              "port": 4201,
              "watch": false
            },
            "configurations": {
              "development": {
                "buildTarget": "dashboard:build:development"
              },
              "production": {
                "buildTarget": "dashboard:build:production"
              }
            },
            "parallelism": true
          }
        },
        "implicitDependencies": []
      }
    },
